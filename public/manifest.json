{
	"manifest_version": 3,
	"name": "Music God",
	"description": "Music Player Extension",
	"version": "1.0",
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"sidePanel"
	],
	"side_panel": {
		"default_path": "index.html"
	},
	"icons": {
		"16": "images/hello_extensions.png",
		"32": "images/hello_extensions.png",
		"192": "images/hello_extensions.png"
	}
}
