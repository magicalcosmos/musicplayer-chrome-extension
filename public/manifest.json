{
	"manifest_version": 3,
	"name": "Music God",
	"description": "Music Player Extension",
	"version": "1.0",
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"sidePanel",
		"tabs"
	],
	"action": {
    "default_title": "Click to open panel"
  },
	"commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+U",
        "mac": "Command+U"
      }
    }
  },
	"icons": {
		"16": "images/hello_extensions.png",
		"32": "images/hello_extensions.png",
		"192": "images/hello_extensions.png"
	}
}
